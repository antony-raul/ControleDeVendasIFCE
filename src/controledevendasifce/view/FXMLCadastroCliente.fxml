<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controledevendasifce.controller.FXMLCadastroClienteController">
    <stylesheets>
        <URL value="@default.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tabelaCliente" layoutY="42.0" prefHeight="376.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="tblColNome" prefWidth="245.0" text="Nome" />
          <TableColumn fx:id="tblColCPF" prefWidth="192.0" text="CPF" />
            <TableColumn fx:id="tblColTelefone" prefWidth="162.0" text="Telefone" />
        </columns>
      </TableView>
      <Button fx:id="btnCadastrar" layoutX="21.0" layoutY="9.0" mnemonicParsing="false" onAction="#janelaCadastrar" text="Cadastrar" />
      <Button fx:id="btnAlterar" layoutX="113.0" layoutY="9.0" mnemonicParsing="false" text="Alterar" />
      <Button fx:id="btnRemover" layoutX="190.0" layoutY="9.0" mnemonicParsing="false" text="Remover" />
   </children>
</AnchorPane>
